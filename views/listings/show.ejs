<% layout("/layouts/boilerplate.ejs") %>
<body>
    <h1>Post Deatails:</h1>
    <ul>
        <li><%= listing.title %></li>
        <li><%= listing.description %></li>
        <li><%= listing.location %></li>
        <li><%= listing.country %></li>
        <li><%= listing.like %></li>
    </ul>
    <br>
    <a href="/listings/<%= listing._id %>/edit">update post</a>
