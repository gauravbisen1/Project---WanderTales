<% layout("/layouts/boilerplate.ejs") %>
<body>
    <h1>All Posts</h1>

    <form method="get" action="/listings/new">
        <button>Create new Post</button>
    </form>

    <ul>
        <% for(let listing of allListings){ %>
            <li>
                <a href="/listings/<%= listing._id %>">
                    <%= listing.title %>
                </a>
            </li>
        <% } %>
    </ul>
</body>
